{
	"name": "isense",
	"description": "Intellisense for JavaScript",
	"publisher": "ntoulasm",
	"author": "ntoulasm",
	"license": "MIT",
	"version": "1.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ntoulasm/isense"
	},
	"categories": [
		"Programming Languages",
		"Linters",
		"Other"
	],
	"keywords": [
		"isense",
		"Intellisense",
		"JavaScript"
	],
	"engines": {
		"vscode": "^1.33.0"
	},
	"activationEvents": [
		"onLanguage:javascript"
	],
	"main": "./client/src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.generateDot",
				"title": "Generate DOT file for the AST of current file"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Example configuration",
			"properties": {
				"languageServerExample.maxNumberOfProblems": {
					"scope": "resource",
					"type": "number",
					"default": 100,
					"description": "Controls the maximum number of problems produced by the server."
				},
				"languageServerExample.trace.server": {
					"scope": "window",
					"type": "string",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"default": "off",
					"description": "Traces the communication between VS Code and the language server."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "cd client && npm run update-vscode && cd .. ",
		"postinstall": "cd client && npm install && cd ../server && npm install && cd .."
	},
	"devDependencies": {
		"@types/mocha": "^5.2.0",
		"@types/node": "^8.0.0",
		"eslint": "^5.13.0",
		"typescript": "^3.8.2"
	},
	"dependencies": {}
}
